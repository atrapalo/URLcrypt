{
    "name": "atrapalo/urlcrypt",
    "description": "PHP library to securely encode and decode short pieces of arbitrary binary data in URLs.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Atrapalo Engineering",
            "email": "dt@atrapalo.com",
            "homepage": "http://atrapalo.github.io",
            "role": "god"
        }
    ],
    "support": {
        "email": "dt_hoteles@atrapalo.com"
    },
    "require": {
        "php": "^7.0 | ^8.0",
        "ext-openssl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 | ^9.5"
    },
    "autoload": {
        "psr-4": {"Atrapalo\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Atrapalo\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
